{
  "name": "test-app",
  "private": true,
  "description": "The better way to handle modals in your Ember.js apps.",
  "repository": "https://github.com/mainmatter/ember-promise-modals",
  "license": "MIT",
  "author": "Mainmatter GmbH",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --env=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto --group",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto --group && pnpm run format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "npm-run-all lint test:*",
    "test:ember-compatibility": "ember try:each"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.28.0",
    "@ember/optional-features": "2.2.0",
    "@ember/string": "3.1.1",
    "@ember/test-helpers": "5.2.2",
    "@embroider/macros": "1.18.0",
    "@embroider/test-setup": "4.0.0",
    "@eslint/js": "9.31.0",
    "@glimmer/component": "1.1.2",
    "babel-plugin-ember-template-compilation": "2.2.5",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "9.2.0",
    "ember-auto-import": "2.10.0",
    "ember-cli": "4.12.3",
    "ember-cli-babel": "8.2.0",
    "ember-cli-dependency-checker": "3.3.3",
    "ember-cli-htmlbars": "6.3.0",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-promise-modals": "workspace:*",
    "ember-qunit": "8.1.1",
    "ember-resolver": "8.1.0",
    "ember-source": "4.12.4",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "7.9.1",
    "ember-try": "3.0.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-ember": "12.5.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.21.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-qunit": "8.2.3",
    "globals": "16.3.0",
    "loader.js": "4.7.0",
    "prettier": "3.6.2",
    "prettier-plugin-ember-template-tag": "2.0.6",
    "qunit": "2.24.1",
    "qunit-console-grouper": "0.3.0",
    "qunit-dom": "2.0.0",
    "sinon": "15.2.0",
    "webpack": "5.100.1"
  },
  "engines": {
    "node": ">= 18.*"
  },
  "ember": {
    "edition": "octane"
  }
}
