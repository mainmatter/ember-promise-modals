{
  "name": "ember-promise-modals",
  "version": "2.0.0",
  "description": "The better way to handle modals in your Ember.js apps.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/simplabs/ember-promise-modals",
  "license": "MIT",
  "author": "simplabs GmbH",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --env=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:md": "prettier --check '**/*.md'",
    "lint:md:fix": "prettier --write '**/*.md'",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "npm-run-all lint test:*",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.1",
    "@embroider/util": "^1.0.0",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-postcss": "^6.0.1",
    "ember-auto-import": "^1.11.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "focus-trap": "^6.7.2",
    "postcss-preset-env": "^7.3.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^1.4.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.1.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.6",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-console-grouper": "^0.3.0",
    "qunit-dom": "^2.0.0"
  },
  "engines": {
    "node": "12.* || >= 14.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "postcss",
        "webpack"
      ]
    }
  }
}
